(defun transpon (m) (apply 'mapcar (cons 'list m)))

(defun koll-rotare  (x)
  (if (>= x 5)
    (- x 9)
    (- x 1)))

(defun t-koll  (koll-str data)
  (transpon (mapcar (function (lambda (el)
                                (mapcar (function (lambda (el1) (list (koll-rotare el) el1)))
                                        (cddr (assoc (strcat koll-str "." (itoa el)) data)))))
                    '(1 2 3 6 7 8))))

(defun t-koll-sort  (koll-str data)
  (mapcar (function (lambda (el) (VL-SORT el (function (lambda (el1 el2) (< (car el1) (car el2)))))))
          (t-koll koll-str data)))

(defun t04-names  (data)
  (mapcar (function (lambda (el) (strcat "t04-" (rtos el)))) (cddr (assoc "t04" data))))

(defun make-t-koll  (koll-str data str-dt-gt)
  (mapcar (function
            (lambda (v-name data / v-str) (setq v-str (strcat str-dt-gt "-" koll-str "-" v-name)) (set (read v-str) data) v-str))
          (t04-names data)
          (t-koll-sort koll-str data)))

